package study;

public class Hello {         //클래스 선언

	public static int sum (int n, int m) {
		return n + m ;
	}
	
	public static void main(String[] args) {
		int i = 20;
		int s;
		char a;
		
		s= sum(i,10); //매소드 호출, i+10
		a = '?';
		
	    System.out.println(a); // char a호출 =?
	    System.out.println("Hello"); // 문자열 hello출력
	    System.out.println(s); // s= sum값 출력
	}
}
